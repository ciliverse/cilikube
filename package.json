{
  "name": "kops",
  "version": "0.1.0",
  "private": true,
  "type": "module", 
  "author": {
    "name": "cillian",
    "email": "cilliandevops@gmail.com",
    "url": "https://github.com/cilliandevops"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "format": "prettier --write src/",
    "build:stage": "vue-tsc --noEmit && vite build --mode staging",
    "build:prod": "vue-tsc --noEmit && vite build",
    "preview:stage": "pnpm build:stage && vite preview",
    "preview:prod": "pnpm build:prod && vite preview",
    "lint:eslint": "eslint --cache --max-warnings 0 \"{src,tests,types}/**/*.{vue,js,jsx,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write \"{src,tests,types}/**/*.{vue,js,jsx,ts,tsx,json,css,less,scss,html,md}\"",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "prepare": "husky install",
    "test": "vitest"
  },
  "dependencies": {
    "@element-plus/icons-vue": "2.1.0",
    "axios": "1.6.2",
    "dayjs": "1.11.10",
    "echarts": "^5.4.3",
    "element-plus": "2.4.2",
    "js-cookie": "3.0.5",
    "json-editor-vue3": "^1.0.8",
    "lodash-es": "4.17.21",
    "mitt": "3.0.1",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "path-browserify": "1.0.1",
    "path-to-regexp": "6.2.1",
    "pinia": "2.1.7",
    "screenfull": "6.0.2",
    "vue": "3.3.8",
    "vue-router": "4.2.5",
    "vxe-table": "4.5.13",
    "vxe-table-plugin-element": "3.1.0",
    "xe-utils": "3.5.14"
  },
  "devDependencies": {
    "@types/js-cookie": "3.0.6",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.9.4",
    "@types/nprogress": "0.2.3",
    "@types/path-browserify": "1.0.2",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "@vitejs/plugin-vue": "4.5.0",
    "@vitejs/plugin-vue-jsx": "3.1.0",
    "@vue/eslint-config-prettier": "8.0.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "@vue/test-utils": "2.4.2",
    "eslint": "8.54.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-vue": "9.18.1",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "lint-staged": "15.1.0",
    "prettier": "3.1.0",
    "sass": "1.69.5",
    "typescript": "5.3.2",
    "unocss": "0.57.7",
    "vite": "5.0.2",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-svg-loader": "5.1.0",
    "vitest": "0.34.6",
    "vue-eslint-parser": "9.3.2",
    "vue-tsc": "1.8.22"
  }
}
